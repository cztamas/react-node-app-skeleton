{
  "name": "react-node-app-skeleton",
  "version": "0.1.0",
  "description": "WIP",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack serve --config webpack.config.js",
    "docker": "docker-compose --env-file .env up -d",
    "lint": "eslint 'src/**/*.js'",
    "start-server": "node src/server/index.js",
    "start-server-dev": "onchange -i -k 'src/**/*.js' '.env' -- node -r dotenv/config src/server/index.js > 'server.log' 2>&1",
    "start-dev": "npm run docker && npm run build",
    "start": "npm run start-dev",
    "stop": "docker-compose down",
    "test:once": "jasmine --config=jasmine.json",
    "test:watch": "onchange -i -k 'src/**/*.js' -- npm run test:once",
    "test": "npm run lint && npm run test:once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cztamas/react-node-app-skeleton.git"
  },
  "author": "cztamas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cztamas/react-node-app-skeleton/issues"
  },
  "homepage": "https://github.com/cztamas/react-node-app-skeleton#readme",
  "dependencies": {
    "express": "4.17.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/preset-env": "7.13.15",
    "@babel/preset-react": "7.13.13",
    "babel-loader": "8.2.2",
    "dotenv": "8.2.0",
    "eslint": "7.25.0",
    "eslint-plugin-jasmine": "4.1.2",
    "eslint-plugin-react": "7.23.2",
    "jasmine": "3.7.0",
    "onchange": "7.1.0",
    "supertest": "6.1.3",
    "webpack": "5.35.1",
    "webpack-cli": "4.6.0",
    "webpack-dev-server": "3.11.2"
  }
}
